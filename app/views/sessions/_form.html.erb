<%= form_with url: sessions_path, local: true do |f| %>
  <%= flash_notice(:alert) %>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email, id: :user_email %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.password_field :password, id: :user_password %>
  </p>

  <p><%= f.submit "Enter" %></p>
<% end %>
